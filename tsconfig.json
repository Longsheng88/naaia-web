{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "lib/utils.js", "lib/db.js", "lib/auth.js", "components/ui/form.js", "components/ui/button.js", "components/ui/toaster.jsx", "components/ui/toast.jsx", "components/ui/label.js", "components/ui/input.js", "components/ui/use-toast.js", "app/api/user/route.js", "app/api/auth/[...nextauth]/route.js", "app/api/route.js", "app/(auth)/sign-up/page.js", "app/(auth)/sign-in/page.js"  ],
  "exclude": [
    "node_modules"
  ]
}
